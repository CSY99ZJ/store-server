{
    "version": 2,
    "builds": [
        {
            "src": "app.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app.js"
        }
    ],
    "engines": {
    "node": "16.15.0",  // 替换为您本地使用的 Node.js 版本
    "npm": "8.5.5"     // 替换为您本地使用的 NPM 版本
}
}
